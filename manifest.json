{
  "manifest_version": 2,
  "name": "Test Ideas Extension",
  "description": "A Browser Extension to help identify new test ideas.",
  "version": "0.0.1",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "web_accessible_resources": ["images/icon.png"]
}
